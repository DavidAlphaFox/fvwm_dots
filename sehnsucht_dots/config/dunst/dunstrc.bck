[global]
font = "Terminus (TTF) 9"
markup = yes
format = "%a: %s\n%b"
sort = yes
indicate_hidden = yes
alignment = center
show_age_threshold = 60
word_wrap = yes
ignore_newline = no
geometry = "400x5-0+0"
transparency = 6
idle_threshold = 120
follow = mouse
sticky_history = yes
line_height = 0
separator_height = 2
padding = 10
horizontal_padding = 10
frame_color = "#ffffff"
separator_color = frame 
startup_notification = false
frame_width = 2
icon_path = /usr/local/share/icons/Adwaita/16x16/status/:/usr/local/share/icons/Adwaita/16x16/devices/
browser=/usr/local/bin/seamonkey

[shortcuts]
close_all = mod4+backslash
history = mod4+grave

[urgency_low]
background = "#2f2f4a"
foreground = "#ffffff"
timeout = 5

[urgency_normal]
background = "#2f2f4a"
foreground = "#f3f4f5"
timeout = 5

[urgency_critical]
background = "#2f2f4a"
foreground = "#E53935"
timeout = 10
